package com.gmail.marialyven12;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] array = new int [] {0, 5, 2, 4, 7, 1, 3, 19};
		int sum = 0;
		
		for (int num: array) {
			if (num % 2 != 0) {
				sum+=1;
			}
		} System.out.println("We have " + sum + " odd elements");
		
		

	}

}
