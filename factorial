package com.gmail.marialyven12;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("Please enter a number bigger than 4 and lower than 16");
		int num = sc.nextInt();
		int fact = 1;
		
		
		if ((num > 4) && (num < 16)) {			
		 for (int i = 1; i <= num; i++) 
			fact = fact * i;
		
			System.out.println("The factorial of the entered number is " + fact);
		} 
		
	}

}
