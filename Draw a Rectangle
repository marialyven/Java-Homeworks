package com.gmail.marialyven12;

public class Main {

	public static void main(String[] args) {
		int a = 3;
		int b = 4;
		drawRectangle(a,b);
		
	}
	public static void drawRectangle(int a, int b) {
		for (int i = 0; i < a; i++) {
			for (int j = 0; j < b; j++) {
				if (i <= a && j <= b) {
					System.out.print("*");
				} else {
					System.out.print(" ");
				}
				

			}
			System.out.println();
		}
	}


